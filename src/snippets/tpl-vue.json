{
  "Vue2-scoped-less": {
    "prefix": "vue2-scoped-less",
    "body": [
      "<template>",
      "\t<div>",
      "\t\tthis is $1",
      "\t</div>",
      "</template>",
      "",
      "<script>",
      "import Vue from 'vue';",
      "export default {",
      "\tname: '$1'",
      "};",
      "</script>",
      "<style scoped lang='less'>",
      "\t.$1{}",
      "</style>",
      ""
    ],
    "description": "Vue2 scoped less style template"
  },
